        <receiver
            android:name="ir.tahasystem.music.app.services.AlarmGPS"
            android:process=":remote"></receiver>

        <service android:name="ir.tahasystem.music.app.services.ServicesSocketMap" android:process=":my_service" />

        <receiver
            android:name="ir.tahasystem.music.app.services.AlarmSocketMap"
            android:process=":remote"></receiver>

